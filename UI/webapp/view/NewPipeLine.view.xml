<mvc:View xmlns:core="sap.ui.core" xmlns:f="sap.f" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="scp.com.saparate.controller.NewPipeLine" xmlns:c="sap.ui.codeeditor">
	<Page xmlns="sap.m" id="idPipeline" floatingFooter="true" class="Height_CSS">
		<headerContent>
			<OverflowToolbar id="otb1">
				<Label text="Project New"/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://favorite"/>
				<Select forceSelection="false" selectedKey="New" change="navigatetoProjectPipeline">
					<core:ListItem key="New" text="New PipeLine"/>
					<core:ListItem key="Import" text="Import PipeLine"/>
				</Select>
			</OverflowToolbar>
			<Toolbar width="100%" id="toolbar2">
				<content></content>
			</Toolbar>
		</headerContent>
		<content>
			<Wizard id="CreatePipeLine">
				<WizardStep id="RepoTypeStep" title="Connect" validated="false">
					<HBox alignItems="Center" justifyContent="Center" width="100%"></HBox>
					<List noDataText="No Repo Types" items="{ path: 'RepoType>/RepoType' }" id="list1" selectionChange="selectRepoType"  mode="SingleSelectLeft">
						<items>
							<StandardListItem title="{RepoType>title}" description="{RepoType>Description}" icon="{RepoType>icon}" id="item15"/>
						</items>
					</List>
					<content/>
				</WizardStep>
				<WizardStep id="RepoStep" validated="false" title="Select">
					<List noDataText="No Repos" items="{ path: 'Repos>/repo' }" id="list2" selectionChange="selectRepo"  mode="SingleSelectLeft">
						<items>
							<ObjectListItem title="{Repos>full_name}" type="Active" press="onRepoItemPress">
								<attributes>
									<ObjectAttribute text="{Repos>branches_url}"/>
									<ObjectAttribute text="{Repos>clone_url}"/>
								</attributes>
							</ObjectListItem>
						</items>
					</List>
					<HBox alignItems="Center" justifyContent="Center" width="100%"></HBox>
				</WizardStep>
				<WizardStep id="branchesStep" validated="false" title="Configure">
						<List noDataText="No Repos" items="{ path: 'branch>/branches' }" id="list3" mode="SingleSelectLeft">
						<items>
							<ObjectListItem title="{branch>name}" type="Active" press="onListItemPress">
								<attributes>
									<ObjectAttribute text="{branch>commit/url}"/>
									<ObjectAttribute text="{branch>commit/sha}"/>
								</attributes>
							</ObjectListItem>
						</items>
					</List>
					<HBox alignItems="Center" justifyContent="Center" width="100%"></HBox>
				</WizardStep>
				<WizardStep id="PricingStep" validated="false" title="Review">
					<c:CodeEditor
						value="specVersion: '1.0' metadata: name: com/saparate type: application builder: customTasks: - name: webide-extension-task-updateNeoApp afterTask: generateVersionInfo configuration: destDir: dist appFolder: webapp nameSpace: scp - name: webide-extension-task-updateManifestJson afterTask: webide-extension-task-updateNeoApp configuration: appFolder: webapp destDir: dist - name: webide-extension-task-lint afterTask: webide-extension-task-updateManifestJson configuration: destDir: dist appFolder: webapp nameSpace: scp - name: webide-extension-task-resources afterTask: webide-extension-task-lint configuration: nameSpace: scp"
						height="300px" type="XML"/>
					<HBox alignItems="Center" justifyContent="Center" width="100%"></HBox>
				</WizardStep>
			</Wizard>
		</content>
	</Page>
</mvc:View>
