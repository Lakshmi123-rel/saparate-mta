<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ObjectListItem id="deploytask" icon="sap-icon://arrow-bottom" type="Inactive"
		title="{ parts : [ {path: 'Stages>stageName'}, {path: 'Stages>type'} ], formatter: '.formatter.deployTaskLabel' }"
		highlight="{ parts : [ 'Stages>status'], formatter: '.formatter.CyclestageStatus' }">
		<attributes>
			<ObjectAttribute text="{Stages>buildJobName}" title="buildJobName"></ObjectAttribute>
			<ObjectAttribute title="buildJobNumber" text="{Stages>buildJobNumber}"/>
			<ObjectAttribute title="Status" text="{Stages>status}"/>
		</attributes>
	
		<firstStatus>
			<ObjectStatus text="{ parts : [ 'Stages>startTime'], formatter: '.formatter.Cyclestagetime' }" title="StartTime"/>
		</firstStatus>
		<secondStatus>
			<ObjectStatus text="{ parts : [ 'Stages>endTime'], formatter: '.formatter.Cyclestagetime' }" title="EndTime"/>
		</secondStatus>
	</ObjectListItem>
</core:FragmentDefinition>