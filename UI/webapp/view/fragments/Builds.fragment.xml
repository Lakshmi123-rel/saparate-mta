<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">

	<Table id="idPipeLineBuildResults" items="{Jobdetails>/}" alternateRowColors="true" >
		<columns>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<Text text="{i18n>pipeline}"/>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<Text text="{i18n>estimatedDuration}"/>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<Text text="{i18n>duration}"/>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<Text text="{i18n>result}"/>
			</Column>
			<Column demandPopin="true" minScreenWidth="Tablet">
				<Text text="{i18n>time}"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem type="Navigation" press="handleSelectionChange" highlight="{parts : [ 'Jobdetails>buildResult'], formatter: '.formatter.buildStatus'}"  >
				<cells>
					<ObjectIdentifier title="{Jobdetails>name}" text="{ parts : [ 'Jobdetails>number'], formatter: '.formatter.addBuild' }"
						class="sapUiTinyMarginTop sapUiTinyMarginBottom"/>
					<ObjectNumber number="{Jobdetails>estimatedDuration}" unit="Sec" state="Information" class="sapUiSmallMargin"/>
					<ObjectNumber number="{Jobdetails>duartion}" unit="Sec" state="Information" class="sapUiSmallMargin"/>
					<GenericTag text="{Jobdetails>buildResult}" status="{ parts : [ 'Jobdetails>buildResult'], formatter: '.formatter.buildStatus' }"
						design="StatusIconHidden"/>
					<ObjectStatus xmlns="sap.m" state="Information" text="{ parts : [ 'Jobdetails>timeStamp'], formatter: '.formatter.getDate' }"
						icon="sap-icon://date-time"/>
				</cells>
			</ColumnListItem>
		</items>
		<headerToolbar>
			<Toolbar width="100%" id="toolbar1">
				<content>
					<Bar id="bar0">
						<contentLeft>
							<Text xmlns="sap.m" id="idBuildstblHdr"/>
						</contentLeft>
						<contentMiddle></contentMiddle>
						<contentRight>
							<Button xmlns="sap.m" text="{i18n>refresh}" press="refreshData" icon="sap-icon://refresh"/>
						</contentRight>
					</Bar>
				</content>
			</Toolbar>
		</headerToolbar>
	</Table>
</core:FragmentDefinition>
